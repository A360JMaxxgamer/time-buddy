<MudTable T="ITimeFrame" Items="TimeFrames">
    <HeaderContent>
        <MudTh>Started at</MudTh>
        <MudTh>Finished at</MudTh>
    </HeaderContent>
    <RowTemplate>
        <MudTd DataLabel="Started at">@context.StartDate</MudTd>
        <MudTd DataLabel="Finished at">@(context.StartDate + context.Duration)</MudTd>
    </RowTemplate>
</MudTable>

@code {
    [Parameter]
    public IEnumerable<ITimeFrame> TimeFrames { get; set; } = Enumerable.Empty<ITimeFrame>();

}